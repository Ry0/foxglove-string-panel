    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "create-foxglove-extension": "1.0.4",
    "eslint": "9.21.0",
    "immer": "10.1.1",
    "lodash": "4.17.21",
    "prettier": "3.5.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@foxglove/schemas": "^1.6.6"
